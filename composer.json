{
  "name": "ldesignmedia/ldesign_laravel_translation",
  "description": "A database translations implementation for Laravel 5.",
  "license": "GPL2",
  "keywords": [
    "laravel",
    "translation",
    "localization",
    "database"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "illuminate/translation": "6.*",
    "illuminate/database": "6.*",
    "illuminate/cache": "6.*",
    "stichoza/google-translate-php": "4.1.*"
  },
  "autoload": {
    "psr-4": {
      "LdesignMedia\\Translation\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LdesignMedia\\Translation\\ServiceProvider"
      ]
    }
  }
}
